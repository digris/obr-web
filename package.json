{
  "name": "openbroadcast.ch",
  "version": "0.0.53",
  "license": "MIT",
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build --base=/static/",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"**/*.{vue,scss}\"",
    "fix": "yarn fix:eslint && yarn fix:stylelint",
    "fix:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "fix:stylelint": "stylelint \"**/*.{vue,scss}\" --fix",
    "schema:generate": "openapi --input http://local.obr-next:8080/api/v1/schema-json/ --output ./src/typings/api/",
    "postinstall": "patch-package"
  },
  "engines" : {
    "node" : ">=16.0.0 <20.0.0"
  },
  "dependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@openreplay/tracker": "^3.5.12",
    "@openreplay/tracker-assist": "^3.5.11",
    "@openreplay/tracker-axios": "^3.5.1",
    "@sentry/tracing": "^6.18.2",
    "@sentry/vue": "^6.18.2",
    "@stripe/stripe-js": "^1.24.0",
    "@vueuse/core": "8.3.1",
    "@vueuse/math": "^9.4.0",
    "@vueuse/router": "^9.4.0",
    "axios": "^0.26.1",
    "bowser": "^2.11.0",
    "color": "^4.2.3",
    "email-validator": "^2.0.4",
    "lodash-es": "^4.17.21",
    "loglevel": "^1.8.0",
    "luxon": "^2.4",
    "mitt": "^3.0.0",
    "mux.js": "^6.0.1",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.0-rc.10",
    "postinstall-postinstall": "^2.1.0",
    "pulltorefreshjs": "^0.1.22",
    "qrcode.vue": "^3.3.3",
    "qs": "^6.10.3",
    "sass": "^1.49.9",
    "shaka-player": "4.3.0",
    "shifty": "^2.17.1",
    "showdown": "^2.1.0",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12",
    "vue3-runtime-template": "^1.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^2.3.0",
    "@types/node": "^16.11.25",
    "@types/node-schedule": "^1.3.2",
    "@types/pulltorefreshjs": "^0.1.5",
    "@types/qs": "^6.9.7",
    "@types/showdown": "^2.0.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "fixed-size-array": "^0.1.1",
    "jsdom": "^19.0.0",
    "openapi-typescript-codegen": "^0.23.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vitest": "^0.5.0",
    "vue-tsc": "^0.31.4"
  }
}
